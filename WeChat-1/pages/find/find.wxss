.nav{
  width:100%;
  height:100rpx;
  display:flex;
  background: #eee;
  flex-direction:row;
}
.default{
  line-height:100rpx;
  text-align:center;
  flex:1;
  border-right:1px solid gainsboro;
  color:#000;
  font-weight:bold;
  font-size:32rpx;
  }
.red{
  line-height:100rpx;
  text-align:center;
  color:#1296db;
  font-size:32rpx;
  flex:1;
  border-right:1px solid gainsboro;
  font-weight:bold;
}
.show{
  display:block;
  text-align:center;
  }
.hidden{
  display:none;
  text-align:center;
  
  }
  .tabar{
    position:absolute;
    top:100rpx;
    left:0;
    bottom:0;
    right:0;
  }
  .tabar>view{
    bottom:0;
    position:absolute;
    display:flex;
    width:100%;
    height:70rpx;
    line-height: 70rpx;
  }
  .tabar>view>view{
    flex:1;
    font-size:36rpx;
    color:white;
  }
  .publish{
    background:orange;
  }
  .search{
    background:skyblue;
  }
.title{
  margin: 14px 0px;
  display: inline-block;
  padding: 0px 8px;
  border-left:3px solid skyblue;
  font-size: 36rpx;
  font-weight: bold;
}
.swiper {
  height: 300rpx;
  
  width: 100%;
}

.swiper image {
  height: 100%;
  width: 100%;
  
}
.swiper-item{
  width: 300rpx;
  background: white;
}

.titles{
  color: #333;
  position: absolute;
  top: 0;
  left: 0;
  color: white;
  font-weight: bold;
  font-size: 36rpx;
  padding: 60rpx 80rpx
}
.re{
  position: relative;
  
}
.alls{
  text-align:right;
  border: 1px solid #eee;
}
.alls view{
   color: #333;
   font-weight: bold;
   display: inline-block;
   padding: 30rpx 40rpx;
}